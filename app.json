{
  "expo": {
    "name": "rn-lecture-base",
    "slug": "rn-lecture-base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/coffee_cup-croped.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-2copy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/coffee_cup-croped.png",
        "backgroundColor": "#c1c1c1"
      },
      "package": "com.hustle2live.rnlecturebase"
    },
    "web": {
      "favicon": "./assets/coffee_cup-croped.png",
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "28848742-d7e8-4d4a-a0c8-cad1de80d973"
      }
    },
    "experiments": {
      "baseUrl": "/react-native-test-task"
    }
  }
}
